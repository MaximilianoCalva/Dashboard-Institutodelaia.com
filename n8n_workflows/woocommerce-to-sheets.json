                        },
                        {
                            "name": "phone",
                            "value": "={{$json[\"billing\"][\"phone\"]}}",
                            "type": "string"
                        }
                    ]
                },
                "options": {}
            },
            "id": "format-data",
            "name": "Format Data",
            "type": "n8n-nodes-base.set",
            "typeVersion": 3.4,
            "position": [
                1100,
                300
            ]
        },
