                            "value": "={{ ($json.line_items || []).map(i => i.name).join(', ') }}",
                            "type": "string"
                        },
                        {
                            "name": "date_created",
                            "value": "={{$json[\"date_created\"]}}",
                            "type": "string"
                        },
                        {
