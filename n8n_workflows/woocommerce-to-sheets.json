            "id": "woocommerce-api",
            "name": "WooCommerce API",
            "type": "n8n-nodes-base.wooCommerce",
            "typeVersion": 1,
            "position": [
                450,
                300
            ],
            "credentials": {
                "wooCommerceApi": {
                    "id": "CREATE_NEW_CREDENTIAL_IN_N8N",
                    "name": "Instituto de la IA Account"
                }
            }
        },
        {
            "parameters": {
                "operation": "read",
                "sheetId": {
                    "__rl": true,
                    "value": "1V36sRqibKoduR078ovbz5SaY3SUXy3b0kdAEM66X-Yg",
                    "mode": "list",
                    "cachedResultName": "Select Spreadsheet"
                },
                "range": "A:A",
                "options": {}
            },
            "id": "read-existing",
            "name": "Read Existing IDs",
            "type": "n8n-nodes-base.googleSheets",
            "typeVersion": 4.5,
            "position": [
                450,
                500
            ],
            "alwaysOutputData": true,
            "credentials": {
                "googleSheetsOAuth2Api": "Google Sheets Account"
            }
        },
        {
            "parameters": {
                "jsCode": "// Explicit Node Reference V13\n// Instead of guessing inputs, we ask for them by name.\n// This prevents \"Self-Duplication\" errors.\n\nlet newOrders = [];\nlet existingData = [];\n\ntry {\n  newOrders = $('WooCommerce API').all();\n} catch(e) {\n  // If run individually, might fail. Fallback to input.all()\n  newOrders = $input.all(); \n}\n\ntry {\n  existingData = $('Read Existing IDs').all();\n} catch(e) {\n  existingData = [];\n}\n\n// 1. Build ID Set from SHEET DATA only\nconst existingIds = new Set();\nexistingData.forEach(item => {\n  const val = item.json.id || item.json.ID || item.json.Id || item.json.values; \n  if (val) existingIds.add(String(val));\n});\n\nconst debugIdsObj = Array.from(existingIds).slice(0, 10).join(', ') + (existingIds.size > 10 ? '...' : '');\n\n// 2. MARK Duplicates\n// We verify newOrders against existingIds\nreturn newOrders.map(item => {\n  const id = item.json.id;\n  item.json._debug_sheet_ids_found = debugIdsObj; // DEBUG INFO\n  \n  // Safety: If existingIds is empty, nothing is duplicate.\n  // Safety: Ensure we aren't comparing the order to itself if inputs got mixed.\n  \n  if (id && existingIds.has(String(id))) {\n     item.json._isDuplicate = true;\n     item.json._processingStatus = \"Duplicate - Skipped\";\n  } else {\n     item.json._isDuplicate = false;\n     item.json._processingStatus = \"New - Processing\";\n  }\n  return item;\n});"
            },
            "id": "mark-duplicates",
            "name": "Check Duplicates (Code)",
            "type": "n8n-nodes-base.code",
            "typeVersion": 2,
            "position": [
                700,
                300
            ]
        },
        {
            "parameters": {
                "conditions": {
                    "boolean": [
                        {
                            "value1": "={{ $json._isDuplicate }}",
                            "value2": false
                        }
                    ]
                }
            },
            "id": "if-new",
            "name": "If New Order",
            "type": "n8n-nodes-base.if",
            "typeVersion": 1,
            "position": [
                900,
                300
            ]
        },
        {
            "parameters": {
                "assignments": {
